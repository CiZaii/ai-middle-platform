# 数据库配置
DB_HOST=your_db_host
DB_PORT=5432
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password
DB_USER=agentx

# 向量数据库配置（默认复用主数据库）
VECTOR_DB_HOST=your_vector_db_host
VECTOR_DB_PORT=5432
VECTOR_DB_USER=agentx
VECTOR_DB_PASSWORD=your_vector_db_password
VECTOR_DB_NAME=agentx
VECTOR_DB_TABLE=public.vector_store
VECTOR_DB_DIMENSION=1024
VECTOR_DB_DROP_TABLE_FIRST=false
VECTOR_DB_CREATE_TABLE=false

RABBITMQ_HOST=your_rabbitmq_host
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=your_rabbitmq_username
RABBITMQ_PASSWORD=your_rabbitmq_password

# LLM服务配置
LLM_DEFAULT_PROVIDER=siliconflow
SILICONFLOW_API_KEY=your_siliconflow_api_key
SILICONFLOW_API_URL=https://api.siliconflow.cn/v1/chat/completions
SILICONFLOW_MODEL=Qwen/Qwen2.5-VL-72B-Instruct
SILICONFLOW_TIMEOUT=30000

# 嵌入模型配置
SILICONFLOW_API_URL_EMBEDDING=https://api.siliconflow.cn/v1/
SILICONFLOW_MODEL_EMBEDDING_MODEL=BAAI/bge-m3

# 重排序模型配置
SILICONFLOW_API_URL_RERANK=https://api.siliconflow.cn/v1/rerank
SILICONFLOW_MODEL_RERANK_MODEL=Pro/BAAI/bge-reranker-v2-m3

# S3对象存储配置（腾讯云COS）
S3_DOMAIN=https://cos.ap-beijing.myqcloud.com
S3_ENDPOINT=cos.ap-beijing.myqcloud.com
S3_SECRET_ID=your_tencent_cloud_secret_id
S3_SECRET_KEY=your_tencent_cloud_secret_key
S3_BUCKET_NAME=your_bucket_name
S3_REGION=cn-beijing
S3_PATH_STYLE_ACCESS=false

# OSS兼容配置（S3配置的别名）
OSS_ACCESS_KEY_ID=your_tencent_cloud_secret_id
OSS_ACCESS_KEY_SECRET=your_tencent_cloud_secret_key
OSS_BUCKET=your_bucket_name

# neo4j配置
NEO4J_URL=bolt://your_neo4j_host:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_neo4j_password

